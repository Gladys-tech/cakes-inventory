{
    "name": "inventory-api",
    "version": "1.0.0",
    "description": "inventory api system",
    "type": "commonjs",
    "main": "index.js",
    "scripts": {
        "dev": "ts-node-dev --respawn src/index.ts",
        "build": "rimraf ./build && tsc",
        "start": "npm run build && node dist/src/index.js",
        "lint": "prettier --check .",
        "format": "npx prettier --write .",
        "prepare": "husky install",
        "configure-husky": "npx husky install && npx husky add .husky/pre-commit \"npx --no-install lint-staged\"",
        "test": "jest",
        "test:coverage": "jest --coverage"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run format && git add ."
        }
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sprintug/inventory-api.git"
    },
    "author": "sprintUG",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/sprintug/inventory-api/issues"
    },
    "homepage": "https://github.com/sprintug/inventory-api#readme",
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.1",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/node": "^20.1.5",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/parser": "^5.59.6",
        "copyfiles": "^2.4.1",
        "faker": "^5.5.3",
        "jest": "^25.0.0",
        "prettier": "^2.8.8",
        "rimraf": "^5.0.0",
        "supertest": "^6.3.3",
        "ts-jest": "^27.0.3",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.0.4"
    },
    "dependencies": {
        "@sendgrid/mail": "^7.7.0",
        "accesscontrol": "^2.2.1",
        "ansi-colors": "^4.1.3",
        "ansi-styles": "^6.2.1",
        "axios": "^1.4.0",
        "bcrypt": "^5.0.1",
        "bull": "^4.10.4",
        "chalk": "^5.2.0",
        "circular-json": "^0.5.9",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "cloudinary": "^1.37.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-rate-limit": "^6.7.0",
        "express-validator": "^7.0.1",
        "form-data": "^4.0.0",
        "handlebars": "^4.7.7",
        "helmet": "^7.0.0",
        "husky": "^8.0.3",
        "jsonwebtoken": "^9.0.0",
        "jwt-decode": "^3.1.2",
        "lodash": "^4.17.21",
        "maven-core-utils": "^1.0.2",
        "multer": "^1.4.5-lts.1",
        "node-cron": "^3.0.2",
        "nodemailer": "^6.9.2",
        "pg": "^8.11.0",
        "redis": "^4.6.6",
        "reflect-metadata": "^0.1.13",
        "sharp": "^0.32.1",
        "streamifier": "^0.1.1",
        "typeorm": "^0.3.16",
        "ulid": "^2.3.0"
    }
}
